<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="product_move_view_form" model="ir.ui.view">
            <field name="name">product.move.form</field>
            <field name="model">product.move</field>
            <field name="arch" type="xml">
                <form string="Product moves">
                    <group col="6">
                        <field name="year"/>
                        <field name="incoming"/>
                        <field name="outgoing"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="product_move_view_tree" model="ir.ui.view">
            <field name="name">product.move.tree</field>
            <field name="model">product.move</field>
            <field name="arch" type="xml">
                <tree string="Product moves">
                    <field name="year"/>
                    <field name="incoming"/>
                    <field name="outgoing"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>
